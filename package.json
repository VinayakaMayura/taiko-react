{
  "name": "taiko-react",
  "version": "0.1.0",
  "description": "React selectors for Taiko",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha \"test/**/*.{js,coffee}\"",
    "test:watch": "mocha --watch --watch-extensions js,coffee \"test/**/*.{js,coffee}\"",
    "build:watch": "coffee -o lib/ -ctw src/",
    "build": "coffee -o lib/ -ct src/"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkshnwesper/taiko-react.git"
  },
  "keywords": [
    "taiko",
    "react"
  ],
  "author": "Saurabh M. Machave",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tkshnwesper/taiko-react/issues"
  },
  "homepage": "https://github.com/tkshnwesper/taiko-react#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "chai": "^4.2.0",
    "coffeescript": "^2.4.1",
    "mocha": "^6.1.2",
    "react": "^16.8.6",
    "sinon": "^7.3.1"
  },
  "dependencies": {
    "resq": "^1.2.1"
  }
